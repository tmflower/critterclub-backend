{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                1
            ]
        },
        "username": {
            "type": "string",
            "minLength": 4,
            "maxLength": 15,
            "default": "",
            "title": "The username Schema",
            "examples": [
                "userExample"
            ]
        },
        "password": {
            "type": "string",
            "minLength": 6,
            "maxLength": 15,
            "default": "",
            "title": "The password Schema",
            "examples": [
                "userPassword"
            ]
        },
        "access_code": {
            "type": "integer",
            "default": 0,
            "title": "The access_code Schema",
            "examples": [
                1234
            ]
        },
        "points": {
            "type": "integer",
            "default": 0,
            "title": "The points Schema",
            "examples": [
                23
            ]
        },
        "parent_id": {
            "type": "integer",
            "default": 0,
            "title": "The parent_id Schema",
            "examples": [
                1
            ]
        }
    },
    "examples": [{
        "id": 1,
        "username": "userExample",
        "password": "123#$abc",
        "access_code": 1234,
        "points": 23,
        "parent_id": 1
    }]
}